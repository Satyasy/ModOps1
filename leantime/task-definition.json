{
    "family": "lks-leantime-app",
    "containerDefinitions": [
        {
            "name": "leantime",
            "image": "leantime/leantime:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "LEAN_SITENAME",
                    "value": "LKS Leantime"
                },
                {
                    "name": "LEAN_DEFAULT_TIMEZONE",
                    "value": "Asia/Jakarta"
                },
                {
                    "name": "LEAN_PORT",
                    "value": "8080"
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "publicUserFiles",
                    "containerPath": "/var/www/html/public/userfiles",
                    "readOnly": false
                },
                {
                    "sourceVolume": "userFiles",
                    "containerPath": "/var/www/html/userfiles",
                    "readOnly": false
                },
                {
                    "sourceVolume": "plugins",
                    "containerPath": "/var/www/html/Plugins",
                    "readOnly": false
                }
            ],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "LEAN_DB_HOST",
                    "valueFrom": "arn:aws:ssm:us-east-1:054802356914:parameter/lks/db/host"
                },
                {
                    "name": "LEAN_DB_PORT",
                    "valueFrom": "arn:aws:ssm:us-east-1:054802356914:parameter/lks/db/port"
                },
                {
                    "name": "LEAN_DB_DATABASE",
                    "valueFrom": "arn:aws:ssm:us-east-1:054802356914:parameter/lks/db/name"
                },
                {
                    "name": "LEAN_DB_USER",
                    "valueFrom": "arn:aws:ssm:us-east-1:054802356914:parameter/lks/db/username"
                },
                {
                    "name": "LEAN_DB_PASSWORD",
                    "valueFrom": "arn:aws:ssm:us-east-1:054802356914:parameter/lks/db/password"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/lks/leantime-app",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::054802356914:role/LabRole",
    "executionRoleArn": "arn:aws:iam::054802356914:role/LabRole",
    "networkMode": "awsvpc",
    "volumes": [
        {
            "name": "publicUserFiles",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-06bd2dd611151b9be",
                "rootDirectory": "/",
                "transitEncryption": "ENABLED",
                "authorizationConfig": {
                    "accessPointId": "fsap-09c0d07cb5f922247",
                    "iam": "ENABLED"
                }
            }
        },
        {
            "name": "userFiles",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-06bd2dd611151b9be",
                "rootDirectory": "/",
                "transitEncryption": "ENABLED",
                "authorizationConfig": {
                    "accessPointId": "fsap-08384947888ee71e5",
                    "iam": "ENABLED"
                }
            }
        },
        {
            "name": "plugins",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-06bd2dd611151b9be",
                "rootDirectory": "/",
                "transitEncryption": "ENABLED",
                "authorizationConfig": {
                    "accessPointId": "fsap-029375f79f884975a",
                    "iam": "ENABLED"
                }
            }
        }
    ],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "enableFaultInjection": false
}